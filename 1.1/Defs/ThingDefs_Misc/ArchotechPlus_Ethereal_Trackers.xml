<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef>
    <defName>ResurrectorTracker</defName>
    <thingClass>ThingWithComps</thingClass>
    <label>Archotech Resurrector Mist</label>
    <category>Ethereal</category>
    <useHitPoints>false</useHitPoints>
    <tickerType>Normal</tickerType>
    <drawerType>None</drawerType>
    <comps>
      <li Class="ArchotechPlus.CompProperties_Resurrector">
      </li>
      <li Class="CompProperties_PlantHarmRadius">
        <radiusPerDayCurve>
          <points>
            <li>0 , 2</li>
            <li>1, 4</li>
          </points>
        </radiusPerDayCurve>
      </li>
      <li Class="CompProperties_ThrownMoteEmitter">
        <mote>Mote_DustSlow</mote>
        <offsetMin>(-.5,0,-.5)</offsetMin>
        <offsetMax>(.5,0,.5)</offsetMax>
        <emissionInterval>15</emissionInterval>
        <burstCount>2</burstCount>
        <scale>0.3~2</scale>
        <rotationRate>-30~30</rotationRate>
        <velocityX>-.25~0.25</velocityX>
        <velocityY>-0.25~0.25</velocityY>
        <colorA>(0.25,0.45,0.75, 1)</colorA>
        <colorB>(0.45,0.6,0.44, 1)</colorB>
      </li>
      <li Class="ArchotechPlus.CompProperties_FollowsTarget">
      </li>
    </comps>
  </ThingDef>
</Defs>
